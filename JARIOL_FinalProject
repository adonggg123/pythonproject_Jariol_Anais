# pythonproject_Jariol_Anais

import turtle

#HEAD
turtle.speed(10)
turtle.fillcolor("pink")
turtle.begin_fill()
turtle.forward(10)
turtle.circle(100,30)
turtle.circle(100,30)
turtle.circle(100,30)
turtle.forward(20)
turtle.circle(100,30)
turtle.circle(100,30)
turtle.circle(100,10)
turtle.circle(160,50)
turtle.circle(100,30)
turtle.circle(100,30)
turtle.right(90)
turtle.circle(40,50)
turtle.circle(50,50)
turtle.circle(50,55)
turtle.circle(150,15)
turtle.forward(45)
turtle.end_fill()

#BODY
turtle.right(100)
turtle.forward(15)
