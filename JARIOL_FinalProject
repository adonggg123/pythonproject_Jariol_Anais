turtle.end_fill()

turtle.penup()
turtle.right(38)
turtle.forward(30)
turtle.right(-9)
turtle.pendown()
turtle.fillcolor("pink")
turtle.begin_fill()
turtle.circle(200,-5)
turtle.circle(120,-30)
turtle.circle(120,-20)
turtle.circle(100,-30)
turtle.circle(50,-50)
turtle.circle(50,-70)
turtle.circle(30,-90)
turtle.forward(-30)
turtle.circle(-30,-40)
turtle.circle(-30,-40)
turtle.circle(-30,-10)
turtle.forward(-43)
turtle.penup()
turtle.end_fill()
turtle.home()

turtle.done()







